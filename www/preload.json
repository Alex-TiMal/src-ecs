{
    "pre-compile": [
        "*-preload.php"
    ],
    "extensions": [
        "php"
    ],
    "files": [
        "vendor/*",
        "config/",
        "public/"
    ],
    "namespace": [
        "App\\Models",
        "App\\Controllers\\",
        "App\\Views\\MainView",
        "Vendor\\Package\\*"
    ],
    "packages": {
        "symfony/http-client": true,
        "vendor/package": {
            "files": true,
            "namespace": true
        }
    },
    "output": "public/preload.php"
}
